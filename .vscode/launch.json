    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Brick Wall",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}\\problems\\brick-wall.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Array Filter TS",
            "program": "${workspaceFolder}/implementations/array-filter.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/out/**/*.js"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Best Time To Buy TS",
            "program": "${workspaceFolder}/problems/121_BestTimeToBuyAndSellStock.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/out/**/*.js"]
        }
    ]
}
